{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier" :["error",{
            // "semi":false ,
            // "singleQuote": true,
             "endOfLine" : "auto"
        }],
        "max-classes-per-file": [
            "error",
            { "ignoreExpressions": true, "max": 3}
        ],
        "extends": "eslint:recommended"
    }
}
